/* pages/canvas/start.wxss */
/* iconfont自动下载的代码 */
@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1584405796268'); /* IE9 */
  src: url('iconfont.eot?t=1584405796268#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1584405796268') format('woff'),
  url('iconfont.ttf?t=1584405796268') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1584405796268#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  letter-spacing:20rpx; 
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-yuandian:before {
  content: "\e61e";
}
.icon-yuandian .active{
  text-shadow: 2rpx 2rpx #000;
}

.icon-huabi::before {
  content: "\e640";
}

.icon-huabi .active{
  font-size: 60rpx;
}

page{
  position: fixed;
}
.container{
  background-color: #ccc;
  position: fixed;
}
/* 整个头部，左边是project，右边是diy */
.head{
  width: 98%;
  height: 80rpx;
  
  margin: 0rpx 20rpx 0rpx 20rpx;
  display: flex;
  justify-content: space-between;
}

.project{
  width: 50%;
  height: 80rpx;
  display: flex;
  
}
.title
{
  height: 40rpx;
  
  border-radius: 20rpx;
  margin: 20rpx 0rpx 0rpx 0rpx;
  line-height: 40rpx;
  text-align: center;
  color: #ff0000;
}
.reload
{
  width: 40rpx;
  height: 40rpx;
  display: flex;
  margin: 20rpx 0rpx;
}
.reload image{
  width: 100%;
  height: 100%;
}


.diy{
  width: 30%;
  height: 80rpx;
  color: #000;
  text-align: center;
  line-height: 80rpx;
}

/* 下面的画布样式 */
.canvasarea {
  width: 96%;
  height: 50%;
  background-color: gray;
  border: 2rpx solid #000;
  margin: 0rpx 15rpx 15rpx 15rpx;
  overflow: hidden;
}

.canvasarea .mycanvas
{
  width:100%;
  height: 100%;
  background-color: #fff;
}

/* 下面是画布操作部分 */
.canvas-caozuo{
  display: flex;
  flex-direction: row;
  height: 120rpx;
  justify-content: space-around;
}

/* 粗细 */
.canvas-weight
{
  height: 120rpx;
  width:90%;
  margin: 0rpx 40rpx;
  display: flex;
  flex-direction: row;
}
.canvas-weight .title
{
  height: 40rpx;
  width:150rpx;
  background-color: #fff;
  border-radius: 20rpx;
  border: 2rpx solid #f00;
  margin: 20rpx 0rpx 0rpx 0rpx;
  line-height: 40rpx;
  text-align: center;
  color: #ff0000;

}

/* 颜色 */
.canvas-color
{
  height: 120rpx;
  width:90%;
  margin: 0rpx 40rpx;
  display: flex;
  flex-direction: row;
  
}
.canvas-color .title
{
  height: 40rpx;
  width:20%;
  background-color: #fff;
  border-radius: 20rpx;
  border: 2rpx solid #f00;
  margin: 20rpx 0rpx 0rpx 0rpx;
  line-height: 40rpx;
  text-align: center;
  color: #ff0000;

}

.right_demo
{
  width: 80%;
  height: 40rpx;
  margin: 10rpx 30rpx;
  
}




/* 弹出框 */
#alert{
  width: 100%;
  height: 100vh;
  background-color: rgba(0,0,0,0.3);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;

}

/* 提交区域 */
#alert-main
{
  height: 480rpx;
  margin: 40rpx ;
  padding: 100rpx 0rpx 0rpx 0rpx;
  background-color: #fff;
  border: 2rpx solid #000;
  border-radius: 30rpx;
}

#frame2
{
  display: flex;
  justify-content: center;
  margin: 35rpx 0rpx;
}
.label
{
  height: 120rpx;
  width: 25%;
  margin: 12rpx 0rpx 10rpx 30rpx;
  font-size: 30rpx;
  text-align: center;
  float: left;
  color: #1296db;
  font-weight: 600;
  line-height: 120rpx;
  box-shadow: 5rpx 5rpx 25rpx #ccc;
  border-radius: 16rpx;
}

.text
{
  height: 120rpx;
  width: 65%;
  margin: 10rpx 20rpx 10rpx 0rpx;
  font-size: 30rpx;
  color:green;
  float: right;
  box-shadow: 5rpx 5rpx 25rpx #ccc;
  border: solid 2rpx #1296db; 
  border-radius: 16rpx;
  text-align: center;
}

.btnline
{
  height: 120rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;

}
.btn
{
  height: 80rpx;
  width:30%;
  margin: 20rpx;
  background-color: #1296db;
  text-align: center;
  line-height: 80rpx;
  color: white;
  font-size: 40rpx;
  box-shadow: 5rpx 5rpx 25rpx #ccc;
  border-radius: 16rpx;
}

/* 发布区域 */
.fabu
{
  height: 200rpx;
  width: 100%;
  /* border: solid 2rpx orange; 搞不清楚的时候就先加框 */
  display: flex;
  justify-content: center;
}

